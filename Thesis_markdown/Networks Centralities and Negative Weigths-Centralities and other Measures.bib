journal = {Soc. Networks},
keywords = {Betweenness,Centrality,Current flow,Random walks},
number = {1},
pages = {39--54},
primaryClass = {cond-mat},
title = {{A measure of betweenness centrality based on random walks}},
volume = {27},
year = {2005}
}
