issn = {1545-2948},
journal = {Annu Rev Genet},
keywords = {*Gene Regulatory Networks,Animals,Genetic Techniques,Humans,Phenotype,Saccharomyces cerevisiae/genetics,Signal Transduction},
pages = {601--625},
pmid = {19712041},
title = {{Systematic mapping of genetic interaction networks}},
