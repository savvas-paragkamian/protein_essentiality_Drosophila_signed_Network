pmid = {15778709},
title = {{Reverse engineering of regulatory networks in human B cells.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=15778709{\&}retmode=ref{\&}cmd=prlinks},
volume = {37},
