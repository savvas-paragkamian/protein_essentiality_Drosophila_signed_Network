issn = {0961-8368},
journal = {Protein Sci.},
keywords = {active sites,are the vertices and,characteristic path length,closeness centrality,conserved central positions,network,networks,org,protein structures can be,proteinscience,represented as small-world,see www,supplemental material,where the amino acids},
number = {9},
pages = {2120--2128},
pmid = {16882992},
title = {{Residue centrality, functionally important residues, and active site shape: analysis of enzyme and non-enzyme families.}},
