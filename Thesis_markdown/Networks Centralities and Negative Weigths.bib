journal = {Philos. Trans. A. Math. Phys. Eng. Sci.},
keywords = {Animals,Biophysics,Biophysics: methods,Evolution,Genetic Engineering,Humans,Molecular,Molecular Biology,Stochastic Processes,Systems Biology,Systems Biology: methods},
number = {1914},
pages = {1125--1139},
pmid = {20123750},
title = {{Biophysics and systems biology}},
url = {http://rsta.royalsocietypublishing.org/content/368/1914/1125.full{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/20123750},
volume = {368},
year = {2010}
}
