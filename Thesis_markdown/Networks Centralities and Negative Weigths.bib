number = {2},
pages = {543--7},
pmid = {11149953},
primaryClass = {nlin},
title = {{A model for the emergence of cooperation, interdependence, and structure in evolving networks}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11149953},
volume = {98},
year = {2001}
}
