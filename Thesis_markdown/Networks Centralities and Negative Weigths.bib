journal = {Proteomics},
keywords = {Essentiality,Localization,Network centrality,Protein interaction network,Systems biology},
number = {22},
pages = {5143--5154},
pmid = {19771559},
title = {{Localized network centrality and essentiality in the yeast-protein interaction network}},
